<h2>New Student Form</h2>
<%= form_tag students_path do %>
    <label> First Name: </label><br>
    <textarea type="text" id="first_name" name="student[first_name]"></textarea><br> 

    <label> Last Name: </label><br>
    <textarea type="text" id="last_name" name="student[last_name]"></textarea><br> 

    <%= hidden_field_tag :authenticity_tocken, form_authenticity_token %>
    <%=submit_tag "Submit Student" %>
<% end %>